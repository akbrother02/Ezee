<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ezee">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Utils.BaseData"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.LoginActivity" />
        <activity android:name=".Activity.ProductActivity" />
        <activity android:name=".Activity.OtpActivity"/>
        <activity android:name=".Activity.SignupActivity" />
        <activity android:name=".Activity.MainCategoryActivity"></activity>
        <activity android:name=".Activity.KnowledgeActivity"/>
        <activity android:name=".Activity.OrderActivity"/>
    </application>

</manifest>